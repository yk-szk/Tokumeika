{
  "name": "tokumeika",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.13",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.25",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.0",
    "@types/react-modal": "^3.10.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-api": "cd api/api && flask run --no-debugger",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-python": "cd api && pyinstaller api_server.spec",
    "build-electron": "cd electron && electron-packager . tokumeika-manager --out build --platform=win32 arch=x64 --electron-version=v9.1.1 --prune --overwrite --ignore test --ignore examples --ignore ignore",
    "package": "yarn build && yarn build-python && yarn build-electron && python package.py local-build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ],
    "*.py": [
      "yapf --in-place"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "proxy": "http://localhost:5000"
}
